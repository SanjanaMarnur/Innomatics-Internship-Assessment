        "marks = [45, 78, 90, 33, 60]\n",
        "pass_count = 0\n",
        "fail_count = 0\n",
        "for i in range(len(marks)):\n",
        "  if(marks[i] >= 50):\n",
        "    pass_count += 1\n",
        "  else:\n",
        "    fail_count += 1\n",
        "\n",
        "print(f\"The number of student passed the exam: {pass_count}\")\n",
        "print(f\"The number of student failed the exam: {fail_count}\")"
      ]
