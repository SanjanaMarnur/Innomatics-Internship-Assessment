        "names = [\" Alice \", \"bob\", \" CHARLIE \"]\n",
        "\n",
        "updated_names = []\n",
        "for name in names:\n",
        "  # strip() --> removes the ending and trailing spaces.\n",
        "  # lower() --> converts to lowercase.\n",
        "  updated_names.append(name.strip().lower())\n",
        "\n",
        "print(updated_names)"
