        "import statistics\n",
        "sales = [1200, 1500, 900, 2300, 1400, 3000]"
      ],
      "metadata": {
        "id": "JlrUE6oiJonO"
      },
      "execution_count": 28,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "avg_sales = statistics.mean(sales)\n",
        "for i, daily_sale in enumerate(sales):\n",
        "  spike = ((daily_sale - avg_sales)/avg_sales)*100\n",
        "  if spike > 30:\n",
        "    print(f\"Day {i+1}: {daily_sale}\")"
      ],
